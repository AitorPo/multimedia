<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidavanzado.asynctasktestmovies_v2">

<uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name="com.androidavanzado.asynctasktestmovies_v2.movies.detailsMovie.view.DetailsMovieActivity"
            android:label="@string/Detalles" />
        <activity
            android:name="com.androidavanzado.asynctasktestmovies_v2.movies.genres.view.GenresActivity"
            android:label="@string/Generos" />
        <activity android:name=".movies.listMovies.view.MovieListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>